{
  "name": "aws-s3-envoltorio",
  "version": "1.0.0",
  "description": "A wrapper for AWS S3 to get presigned url to get and put objects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Shubham-Somani@github.com/Shubham-Somani/aws-s3.git"
  },
  "keywords": [
    "aws",
    "aws s3",
    "aws-s3",
    "s3",
    "aws s3 presigned url",
    "presigned url"
  ],
  "author": "Shubham Somani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Shubham-Somani/aws-s3/issues"
  },
  "homepage": "https://github.com/Shubham-Somani/aws-s3#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "dotenv": "^8.5.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.14.0",
    "@aws-sdk/client-s3": "^3.14.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.14.0",
    "@aws-sdk/s3-request-presigner": "^3.14.0",
    "@aws-sdk/smithy-client": "^3.13.1",
    "@aws-sdk/types": "^3.13.1"
  },
  "bundledDependencies": false
}
